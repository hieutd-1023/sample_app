<% provide :title, t(".sign_up") %>
<% provide :button_text, t(".sign_up") %>
<h1 class="signup-title"><%= t(".sign_up") %></h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |f| %>
      <%= render "shared/error_messages", object: f.object %>
    <% end %>
  </div>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render "form" %>
  </div>
</div>
